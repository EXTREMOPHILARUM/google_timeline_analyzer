# Google Places API Configuration
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Database Configuration (used by docker-compose)
DATABASE_URL=postgresql://timeline_user:timeline_pass@postgres:5432/timeline_analyzer

# Redis Configuration
REDIS_URL=redis://redis:6379

# Optional: Rate limiting for Places API (requests per second)
PLACES_API_RATE_LIMIT=100

# Optional: Cache TTL in seconds (default: 30 days)
CACHE_TTL=2592000
